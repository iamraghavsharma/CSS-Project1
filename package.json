{
  "name": "CSS-Project1",
  "version": "1.0.0",
  "description": "Landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamraghavsharma/CSS-Project1.git"
  },
  "author": "Raghavendra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamraghavsharma/CSS-Project1/issues"
  },
  "homepage": "https://github.com/iamraghavsharma/CSS-Project1#readme",
  "dependencies": {
    "jquery": "^3.5.1",
    "live-server": "^1.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.1",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1"
  }
}
